oductList\n  })\n\n  const shopName = computed(() => {\n    const shopName = cartList[shopId]?.shopName || ''\n    return shopName\n  })\n\n  const calculations = computed(() => {\n    const productList = cartList[shopId]?.productList\n    const result = { total: 0, price: 0, allChecked: true}\n    if(productList) {\n       for(let i in productList) {\n         const product = productList[i]\n         result.total += product.count\n         if(product.check) {\n           result.price += (product.count * product.price)\n         }\n         if(product.count > 0 && !product.check) {\n           result.allChecked = false\n         }\n      }\n    }\n    result.price = result.price.toFixed(2)\n    return result\n  })\n\n  return { cartList, shopName, productList, calculations, changeCartItemInfo }\n}"],"mappings":"AAAA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B;AACA,OAAO,MAAMC,mBAAmB,GAAIC,MAAM,IAAK;EAC7C,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;EACxB,MAAMI,QAAQ,GAAGD,KAAK,CAACE,KAAK,CAACD,QAAQ;EACrC,MAAME,kBAAkB,GAAGA,CAACJ,MAAM,EAAEK,SAAS,EAAEC,WAAW,EAAEC,GAAG,KAAK;IAClEN,KAAK,CAACO,MAAM,CAAC,oBAAoB,EAAE;MACjCR,MAAM;MAAEK,SAAS;MAAEC,WAAW;MAAEC;IAClC,CAAC,CAAC;EACJ,CAAC;EAED,MAAME,WAAW,GAAGZ,QAAQ,CAAC,MAAM;IAAA,IAAAa,gBAAA;IACjC,MAAMD,WAAW,GAAG,EAAAC,gBAAA,GAAAR,QAAQ,CAACF,MAAM,CAAC,cAAAU,gBAAA,uBAAhBA,gBAAA,CAAkBD,WAAW,KAAI,CAAC,CAAC;IACvD,MAAME,mBAAmB,GAAG,CAAC,CAAC;IAC9B,KAAI,IAAIC,CAAC,IAAIH,WAAW,EAAE;MACxB,MAAMI,OAAO,GAAGJ,WAAW,CAACG,CAAC,CAAC;MAC9B,IAAGC,OAAO,CAACC,KAAK,GAAG,CAAC,EAAG;QACrBH,mBAAmB,CAACC,CAAC,CAAC,GAAGC,OAAO;MAClC;IACF;IACA,OAAOF,mBAAmB;EAC5B,CAAC,CAAC;EAEF,MAAMI,QAAQ,GAAGlB,QAAQ,CAAC,MAAM;IAAA,IAAAmB,iBAAA;IAC9B,MAAMD,QAAQ,GAAG,EAAAC,iBAAA,GAAAd,QAAQ,CAACF,MAAM,CAAC,cAAAgB,iBAAA,uBAAhBA,iBAAA,CAAkBD,QAAQ,KAAI,EAAE;IACjD,OAAOA,QAAQ;EACjB,CAAC,CAAC;EAEF,MAAME,YAAY,GAAGpB,QAAQ,CAAC,MAAM;IAAA,IAAAqB,iBAAA;IAClC,MAAMT,WAAW,IAAAS,iBAAA,GAAGhB,QAAQ,CAACF,MAAM,CAAC,cAAAkB,iBAAA,uBAAhBA,iBAAA,CAAkBT,WAAW;IACjD,MAAMU,MAAM,GAAG;MAAEC,KAAK,EAAE,CAAC;MAAE